package ExceptionHandling;

public class InterruptedExc {
	void stuff() throws InterruptedException{
	doStuff();	
	}
	
	void doStuff() throws InterruptedException{
	domorestuff();	
	}
	
	void domorestuff() throws InterruptedException{
		Thread.sleep(1000);
	}
public static void main(String[] args) {
	InterruptedExc i1=new InterruptedExc();
	i1.stuff();
	
}
}
