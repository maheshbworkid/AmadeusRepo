package testpractice;

public class MaxItemInArray {
	static void maxItem(int arr[]) {
		int max=arr[0];
		for (int i=1; i<arr.length; i++) {
			if(arr[i]>max) {
				max=arr[i];//3>1 max=3 
			}
			
		}
		System.out.println(max);
	}
public static void main(String[] args) {
	int arr[]= {1,3,23,9,189};
	maxItem(arr);
}
}
