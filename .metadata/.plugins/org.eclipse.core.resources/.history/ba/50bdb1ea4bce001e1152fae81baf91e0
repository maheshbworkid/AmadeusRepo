package Concepts;

public class StringObjectCreation {
	
	
public static void main(String[] args) {
	String  st=new String("test");
	System.out.println(st);
	st=st.concat("post");
	System.out.println(st);
	
	StringBuffer sb=new StringBuffer("test");
	sb.append("post");
	System.out.println(sb);
	
	
}
}
